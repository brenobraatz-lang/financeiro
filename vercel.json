{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ]
  ,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/cliente/ativos/$1" },
    { "src": "/logo.webp", "dest": "/cliente/logo.webp" },
    { "src": "/vite.svg", "dest": "/cliente/vite.svg" },
    { "src": "/(.*)", "dest": "/cliente/index.html" }
  ]
}
